<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OnlyAssignInProjectDates</fullName>
    <active>false</active>
    <errorConditionFormula>IF(ProjectLineItem__r.Project__r.StartProject__c &gt; StartDateResource__c ||  ProjectLineItem__r.Project__r.EndProject__c &lt;  EndDateResource__c , true, false)</errorConditionFormula>
    <errorMessage>The dates entered do not correspond to the project</errorMessage>
</ValidationRule>
